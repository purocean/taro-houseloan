		"@tarojs/router": "1.3.37",
		"@tarojs/taro": "1.3.37",
		"@tarojs/taro-alipay": "1.3.37",
		"@tarojs/taro-h5": "1.3.37",
		"@tarojs/taro-swan": "1.3.37",
		"@tarojs/taro-tt": "1.3.37",
		"@tarojs/taro-weapp": "1.3.37",
		"base64-utf8": "^0.1.8",
		"dayjs": "^1.8.35",
		"mobx": "4.8.0",
		"nerv-devtools": "^1.5.6",
		"nervjs": "^1.5.6"
	},
	"devDependencies": {
		"@tarojs/cli": "1.3.37",
		"@tarojs/plugin-babel": "1.3.37",
		"@tarojs/plugin-csso": "1.3.37",
